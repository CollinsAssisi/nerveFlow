    "import numpy as np\n",
    "\n",
    "for n,i in enumerate([\"state.batch\"+str(n+1)+\".npy\" for n in range(5)]):\n",
    "    if n<1:\n",
    "        l=np.load(i)\n",
    "    else:\n",
    "        l = np.concatenate([l,np.load(i)[1:,:]])\n",
    "\n",
    "np.savetxt(\"state.vector\",l,delimiter=\",\",fmt=\"%.3f\")"
   ]
